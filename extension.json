{
	"name": "WikiApiary",
	"version": "0.5.2",
	"author": [
		"[https://www.mediawiki.org/wiki/User:Cindy.cicalese Cindy Cicalese]",
		"[https://www.mediawiki.org/wiki/User:Sen-Sai Charly Cobben]"          
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikiApiary",
	"descriptionmsg": "wikiapiary-desc",
	"type": "parserhook",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"MessagesDirs": {
		"WikiApiary": [
			"i18n",
			"i18n/api"
		]
	},
	"ExtensionMessagesFiles": {
		"WikiApiaryMagic": "i18n/WikiApiary.i18n.php"
	},
	"AutoloadClasses": {
		"W8yAPI": "W8yAPI.php"
	},
	"APIModules": {
		"wikiapiary": "W8yAPI"
	},
	"AutoloadNamespaces": {
		"WikiApiary\\": "includes/"
	},
	"HookHandlers": {
		"dbhooks": {
			"class": "WikiApiary\\DBHooks"
		}
	},
	"Hooks": {
		"LoadExtensionSchemaUpdates": "dbhooks",
		"ParserFirstCallInit": "dbhooks",
		"ScribuntoExternalLibraries": "dbhooks"
	},
	"config": {
		"WikiApiary": {
			"value": {
				"debug": false
			}
		}
	},
	"manifest_version": 2
}
